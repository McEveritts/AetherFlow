{
    "name": "aetherflow/dashboard",
    "description": "AetherFlow Seedbox Dashboard",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "McEveritts",
            "email": "armyworkbs@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "league/oauth2-google": "^4.0",
        "vlucas/phpdotenv": "^5.6",
        "monolog/monolog": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "filp/whoops": "^2.16"
    },
    "config": {
        "platform": {
            "php": "8.1"
        }
    },
    "scripts": {
        "cs-check": "php-cs-fixer fix --dry-run --diff",
        "cs-fix": "php-cs-fixer fix"
    },
    "autoload": {
        "psr-4": {
            "AetherFlow\\": "inc/"
        }
    }
}